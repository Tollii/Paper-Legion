<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.loginController">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" StackPane.alignment="CENTER">
         <center>
            <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <center>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="BOTTOM_CENTER">
                     <children>
                        <JFXTextField fx:id="usernameInput" labelFloat="true" promptText="username" />
                        <JFXPasswordField fx:id="passwordInput" labelFloat="true" promptText="password">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </JFXPasswordField>
                        <BorderPane prefHeight="75.0" prefWidth="200.0">
                           <right>
                              <VBox alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="100.0" BorderPane.alignment="BOTTOM_RIGHT">
                                 <children>
                                    <Label fx:id="newUserButton" contentDisplay="RIGHT" text="New User?">
                                       <VBox.margin>
                                          <Insets top="5.0" />
                                       </VBox.margin>
                                       <font>
                                          <Font size="9.0" />
                                       </font>
                                    </Label>
                                    <Label fx:id="forgotPasswordButton" text="Forgot password?">
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                       <font>
                                          <Font size="9.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </VBox>
                           </right>
                           <bottom>
                              <JFXButton fx:id="enterButton" defaultButton="true" text="Enter" BorderPane.alignment="CENTER" />
                           </bottom>
                        </BorderPane>
                     </children>
                     <BorderPane.margin>
                        <Insets left="20.0" right="20.0" top="100.0" />
                     </BorderPane.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </VBox>
               </center>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </BorderPane>
         </center>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </BorderPane>
   </children>
</StackPane>
